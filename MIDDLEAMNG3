	LDA 8000
	MOV B,A 
	LDA 8001
	CMP B
	JC BGRT1
	MOV B,A
	LDA 8002
	CMP B
	JC BGRT2
  MOVE: MOV A,B
 STORE: STA 8003
	JMP END
 BGRT2: LDA 8000
	MOV B,A
	LDA 8002
	CMP B
	JC BGRT3 
	JMP STORE
 BGRT3: JMP MOVE
 BGRT1: LDA 8002
	CMP B
	JC BGRT4
	JMP MOVE
 BGRT4: LDA 8001
	MOV B,A
	LDA 8002
	CMP B
	JC BGRT5
	JMP STORE
 BGRT5: JMP MOVE
   END: HLT